@ARTICLE{Werden1994,
  author = {Werden, Gregory J and Froeb, Luke M},
  title = {The Effects of Mergers in Differentiated Products Industries: Logit
	Demand and Merger Policy},
  journal = {Journal of Law, Economics and Organization},
  year = {1994},
  volume = {10},
  pages = {407-26},
  number = {2},
  month = {October},
  abstract = { Using the logit model, and assuming Nash equilibrium in prices and
	constant marginal cost, it is straightforward to estimate critical
	demand parameters and simulate mergers. In this way, the logit model
	can be used to predict the price and welfare effects of mergers.
	We explore the effects of mergers in this model both analytically
	and through simulations of hypothetical mergers of U.S. long distance
	carriers. We find that only mergers involving AT\&T would be likely
	to lessen welfare significantly. Simulations such as these provide
	a firmer foundation for antitrust policy than traditional structural
	indicators. The logit model is not always appropriate, but the basic
	methodology can be adapted to other demand systems. Copyright 1994
	by Oxford University Press.},
  owner = {taragic},
